{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.py"
    }
  ],
  "env": {
    "OPENSUBTITLES_USERNAME": "@opensubtitles_username",
    "OPENSUBTITLES_PASSWORD": "@opensubtitles_password",
    "SESSION_SECRET": "@session_secret"
  }
}

